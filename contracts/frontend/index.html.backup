            padding: 10px;
            border-radius: 8px;
            margin-bottom: 25px;
            word-break: break-all;
            color: #666;
            border-left: 3px solid #00ff88;
        }

        /* Form Elements */
        .input-group {
            margin: 20px 0;
        }

        .input-group label {
            display: block;
            margin-bottom: 8px;
            font-weight: 600;
            color: #00ff88;
            font-size: 0.95rem;
        }

        .input-group input,
