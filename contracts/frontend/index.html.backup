            text-transform: uppercase;
            letter-spacing: 1px;
            font-family: inherit;
            display: inline-flex;
            align-items: center;
            gap: 8px;
        }

        .btn:hover {
            transform: translateY(-2px);
            box-shadow: 0 10px 30px rgba(0,255,136,0.3);
        }

        .btn:active {
            transform: translateY(0);
        }

        .btn:disabled {
            opacity: 0.5;
            cursor: not-allowed;
            transform: none;
        }

        .btn-secondary {
            background: transparent;
            border: 1px solid #333;
            color: #fff;
        }

        .btn-secondary:hover {
            border-color: #00ff88;
            background: rgba(0,255,136,0.1);
        }

        .btn-small {
            padding: 10px 20px;
            font-size: 0.9rem;
        }

        /* Messages */
        .message {
            margin-top: 15px;
            padding: 12px;
