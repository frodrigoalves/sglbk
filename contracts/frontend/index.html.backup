    <!-- Stats Section -->
    <section class="stats">
        <div class="stats-grid">
            <div class="stat-item">
                <h3>10K+</h3>
                <p>Avatares Criados</p>
            </div>
            <div class="stat-item">
                <h3>50K+</h3>
                <p>Cápsulas do Tempo</p>
            </div>
            <div class="stat-item">
                <h3>99.9%</h3>
                <p>Uptime Blockchain</p>
