            outline: none;
            border-color: #00ff88;
            box-shadow: 0 0 20px rgba(0,255,136,0.2);
            background: rgba(255,255,255,0.08);
        }

        .input-group input::placeholder,
        .input-group textarea::placeholder {
            color: #666;
        }

        /* Buttons */
        .btn {
            background: linear-gradient(45deg, #00ff88, #00ccff);
            border: none;
            color: #000;
            padding: 14px 28px;
            border-radius: 10px;
            cursor: pointer;
            font-size: 1rem;
            font-weight: 600;
            margin: 8px;
